{
  "extends": "eslint-config-airbnb",
  "rules": {
    "import/extensions": 0,
    "import/no-extraneous-dependencies": 0,
    "import/no-unresolved": [2, { "ignore": [] }],
    "linebreak-style": 0,
    "react/prefer-stateless-function": 0,
    "quotes": [1, "single"],
    "comma-dangle": ["error", "never"],
    "no-console": "off",
    "no-unused-vars": "warn"
  },
  "parserOptions": {
    "ecmaFeatures": {
      "modules": true
    }
  }
}
